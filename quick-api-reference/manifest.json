{
  "manifest_version": 3,
  "name": "Quick API Reference",
  "version": "1.0",
  "description": "A quick reference for various APIs.",
  "icons": {
    "16": "icons/icon-16.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },

  "permissions": ["storage", "alarms"],
  "minimum_chrome_version": "102",
  "omnibox": {
    "keyword": "api"
  }, 
  "content_scripts": [{
    "matches": ["https://developer.chrome.google.cn/*"],
    "js": ["content-script.js"]
  }]
}